<view class="recommend">
  <block wx:for="{{recommends}}" wx:key="index">
    <view class="recommend-item">
      <image src="{{item.img1v1Url}}" class="item-img"/>
      <view><text class="title">{{item.name}}</text></view>
    </view>
  </block>
</view>
