/* pages/profile/childcpns/songList.wxss */
.songList {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}
.songList .imgContainer {
  width: 100rpx;
  height: 100rpx;
  margin-right: 30rpx;
}
.songList .imgContainer .img {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}
.songList .textContainer .title {
  font-size: 28rpx;
  margin-bottom: 6rpx;
}
.songList .textContainer .desc {
  font-size: 24rpx;
  color: #999;
}